<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav"
    app:startDestination="@id/menu">

    <fragment
        android:id="@+id/menu"
        android:name="com.example.examen1.Menu"
        android:label="fragment_menu"
        tools:layout="@layout/fragment_menu" >
        <action
            android:id="@+id/go_alcuadrado"
            app:destination="@id/alcuadrado" />
        <action
            android:id="@+id/go_raizcuadrada"
            app:destination="@id/go_raizcuadrada" />
        <action
            android:id="@+id/go_antecesor"
            app:destination="@id/antecesor" />
        <action
            android:id="@+id/go_sucesor"
            app:destination="@id/sucesor" />
    </fragment>
    <fragment
        android:id="@+id/alcuadrado"
        android:name="com.example.examen1.alcuadrado"
        android:label="fragment_alcuadrado"
        tools:layout="@layout/fragment_alcuadrado" />
    <fragment
        android:id="@+id/go_raizcuadrada"
        android:name="com.example.examen1.Raizcuadrada"
        android:label="fragment_raizcuadrada"
        tools:layout="@layout/fragment_raizcuadrada" />
    <fragment
        android:id="@+id/antecesor"
        android:name="com.example.examen1.Antecesor"
        android:label="fragment_antecesor"
        tools:layout="@layout/fragment_antecesor" />
    <fragment
        android:id="@+id/sucesor"
        android:name="com.example.examen1.Sucesor"
        android:label="fragment_sucesor"
        tools:layout="@layout/fragment_sucesor" />
</navigation>